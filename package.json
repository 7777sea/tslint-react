{
  "name": "tslint-react",
  "version": "0.1.0",
  "description": "React-specific rules for TSLint",
  "repository": {
    "type": "git",
    "url": "https://github.com/adidahiya/tslint-react.git"
  },
  "license": "Apache-2.0",
  "scripts": {
